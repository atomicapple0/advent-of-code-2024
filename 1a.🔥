def main():
    with open("./inputs/1a.input", "r") as file:
        s = file.read()
    lines = s.splitlines(keepends=False)

    var ls = List[Int]()
    var rs = List[Int]()
    for line in lines:
        words = line[].split("   ")
        l = int(words[0])
        r = int(words[1])
        ls.append(int(l))
        rs.append(int(r))
        # print(str(l) + "  " + str(rs[i]))

    sort(ls)
    sort(rs)

    acc = 0
    for i in range(len(ls)):
        acc += abs(ls[i] - rs[i])

    print(acc)
